{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {}
    }
  ],
  "headers": [
    {
      "source": "/_next/static/(?:[^/]+/pages|pages)/.*",
      "headers": [
        {
          "key": "cache-control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/static/.*",
      "headers": [
        {
          "key": "cache-control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    }
  ]
}
